{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@data/*": ["src/data/*"],
      "@content/*": ["src/content/*"],
      "@utils/*": ["src/utils/*"],
      "@types": ["src/types"]
    }
  }
}
